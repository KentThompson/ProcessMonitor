{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "prisma generate",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "prebuild": "npx prisma generate --schema=prisma/schema.prisma && prisma migrate dev --name init",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "generate": "^0.10.0",
    "install": "^0.13.0",
    "npm": "^11.6.2",
    "npx": "^3.0.0",
    "prisma": "^6.16.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.0"
  }
}
